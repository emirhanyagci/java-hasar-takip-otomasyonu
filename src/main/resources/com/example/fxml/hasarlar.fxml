<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="507.0" prefWidth="566.0" spacing="10" style="-fx-padding:10;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java_proje_fx.controller.HasarlarController">
    <Label style="-fx-font-size:16; -fx-font-weight:bold;" text="Hasar Detayları" />
    <TableView fx:id="hasarTable">
      <columns>
         <TableColumn prefWidth="110.0" text="Damage Details" />
         <TableColumn prefWidth="40.0" text="ID" />
         <TableColumn prefWidth="75.0" text="Status" />
         <TableColumn prefWidth="75.0" text="Tarih" />
         <TableColumn prefWidth="75.0" text="Costumer" />
      </columns></TableView>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button text="Yeni Hasar Oluştur">
            <HBox.margin>
               <Insets left="5.0" right="5.0" />
            </HBox.margin>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         </Button>
         <Button layoutX="10.0" layoutY="10.0" text="Rapor Ekle">
            <HBox.margin>
               <Insets left="5.0" right="5.0" />
            </HBox.margin>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         </Button>
      </children>
   </HBox>
</VBox>
